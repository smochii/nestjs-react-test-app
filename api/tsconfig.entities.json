{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2015",
    "sourceMap": true,
    "outDir": "./dist/entities",
    "baseUrl": "./",
    "incremental": false,
    "esModuleInterop": true
  },
  "include": [
    "src/entities/*"
  ]
}
